{
  "name": "internal-gutenberg",
  "version": "1.0.0",
  "description": "Webhooks.",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf ./dist",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "npm run test:startdb; TEST_ENV=true AWS_REGION=us-west-2 WEBHOOK_TABLE=GutenbergTable EVENT_QUEUE=doesntmatterfortesting AWS_ACCESS_KEY_ID=AKID AWS_SECRET_ACCESS_KEY=SECRET mocha --recursive --throw-deprecation --require ts-node/register --require ./src/utils/test/setLogLevelSilent.ts --exit \"src/**/*.test.ts\"; npm run test:stopdb",
    "test:debug": "npm run test:startdb; TEST_ENV=true AWS_REGION=us-west-2 WEBHOOK_TABLE=GutenbergTable mocha --recursive --throw-deprecation --require ts-node/register --require ./src/utils/test/setLogLevelDebug.ts --exit \"src/**/*.test.ts\"; npm run test:stopdb",
    "test:startdb": "docker start gutenberg-test-db || (docker run --name gutenberg-test-db -p 8000:8000 amazon/dynamodb-local)",
    "test:stopdb": "docker stop gutenberg-test-db"
  },
